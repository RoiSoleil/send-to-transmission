{
	"name":"Send to Transmission",
	"description":"Add, control .torrent in Tranmission",
	"version":"0.1.0",
	"manifest_version":2,
	"permissions":[
		"contextMenus",
		"notifications",
		"http://*/"
	],
	"background":{
		"page":"html/background.html"
	},
	"options_page":"html/options.html",
	"icons":{
		"16":"img/16.png",
		"48":"img/48.png",
		"128":"img/128.png"
	},
	"browser_action":{
		"default_icon":"img/16.png",
		"default_title":"Send to Transmission",
		"default_popup":"html/view.html"
	},
	"web_accessible_resources":[
		"img/128.png"
	]
}